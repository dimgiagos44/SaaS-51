        script(src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js")
        canvas#myChart(style='width:100%;max-width:700px') 
        - var xValues = labels;
        - var yValues = values;
        script. 
            
            var barColors = ["red", "green","blue","orange","brown", "green", "blue", "black", "yellow"];

            new Chart("myChart", {
            type: "bar",
            data: {
                labels: xValues,
                datasets: [{
                backgroundColor: barColors,
                data: yValues
                }]
            },
            options: {}
            });
        